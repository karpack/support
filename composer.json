{
    "name": "karpack/support",
    "description": "Common support module",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/karpack/support/issues",
        "source": "https://github.com/karpack/support"
    },
    "keywords": ["laravel", "support"],
    "authors": [
        {
            "name": "Kalesh Kaladharan",
            "email": "kalesh.13@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/container": ">=7.0",
        "illuminate/database": ">=7.0",
        "illuminate/support": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Karpack\\Support\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Karpack\\Support\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
